{
  "compilerOptions": {
    "outDir": "lib",
    "module": "ESNext",
    "lib": ["es6", "dom"],
    "inlineSourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "types": ["jest"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  }
}
